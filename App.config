		<!-- 119 is the virtual key code for F8 -->
		<add key="AnnotationFontSize" value="14" />
		<add key="AnnotationSymbolOffsetX" value="15" />
		<add key="AnnotationSymbolOffsetY" value="15" />
		
	</appSettings>
</configuration>